{
	"name": "ecomailcz/raynet-client",
	"description": "Basic client for working with Raynet API",
	"author": "Jan Tlap√°k (jan@ecomail.cz)",
	"license": ["MIT"],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "~7.1"
	},
	"require-dev": {
		"slevomat/coding-standard": "^4.6",
		"phpstan/phpstan": "^0.10.0",
		"phpunit/phpunit": "^7.2",
		"phing/phing": "^2.16",
		"fzaninotto/faker": "^1.7",
		"roave/security-advisories": "dev-master",
		"sllh/composer-versions-check": "^2.0",
		"wapmorgan/php-code-fixer": "^2.0",
		"sebastian/phpcpd": "^4.0"
	},
	"autoload": {
		"psr-4": {
			"EcomailRaynet\\": ["src/"],
			"EcomailRaynetTest\\": ["tests/"]
		}
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	}
}